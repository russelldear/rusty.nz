{
  "manifest_version": 2,
  "name": "rusty.nz",
  "short_name": "rusty.nz",
  "description": "Shorten urls using rusty.nz",
  "version": "1.1",
  "permissions": [
    "contextMenus",
    "tabs", "http://*/*", "https://*/*", "<all_urls>"
  ],
  "background": {
    "scripts": [
      "content.js"
    ]
  }
}